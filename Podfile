source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '9.3'
use_frameworks!

def connect_pods
    pod 'ObjectMapper'
    pod 'SwiftyJSON'
    pod 'Gloss', '~> 1.2'
end

target "JsonSerializerComparison" do
    connect_pods
end

target "JsonSerializerComparisonTests" do
    connect_pods
end

